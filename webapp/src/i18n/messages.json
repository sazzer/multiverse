{
  "page": {
    "title": "Multiverse",
    "spinner": {
      "label": "Loading..."
    },
    "pagination": {
      "label": "Page navigation",
      "next": "Goto next page",
      "previous": "Goto previous page",
      "page": "Goto page {{page}}"
    },
    "pageCount": "{{first}} to {{last}} of {{total}}",
    "errors": {
      "unexpected": "An unexpected error occurred"
    }
  },
  "header": {
    "toggleNavigation": "Toggle Navigation",
    "userMenu": {
      "label": "User menu: {{user}}",
      "profile": "User Profile",
      "newWorld": "New World",
      "logout": "Log Out"
    }
  },
  "authentication": {
    "start": {
      "title": "Login / Register",
      "submit": "Login / Register"
    },
    "login": {
      "title": "Login",
      "submit": "Login",
      "cancel": "Cancel"
    },
    "register": {
      "title": "Register",
      "submit": "Register",
      "cancel": "Cancel"
    },
    "username": {
      "label": "Username",
      "errors": {
        "required": "Please enter a username",
        "pattern": "$t(authentication.username.errors.required)"
      }
    },
    "email_address": {
      "label": "Email Address",
      "errors": {
        "required": "Please enter an email address",
        "pattern": "$t(authentication.email_address.errors.required)"
      }
    },
    "display_name": {
      "label": "Display Name"
    },
    "password": {
      "label": "Password",
      "errors": {
        "required": "Please enter a password",
        "pattern": "$t(authentication.password.errors.required)"
      }
    },
    "password2": {
      "label": "Repeat Password",
      "errors": {
        "required": "Please re-enter your password",
        "noMatch": "Passwords do not match"
      }
    },
    "errors": {
      "invalidPassword": "Invalid username or password",
      "duplicateUsername": "That username is already registered"
    }
  },
  "profile": {
    "title": "User Profile: {{name}}",
    "profile": {
      "label": "Profile",
      "submit": "Save Changes",
      "cancel": "Cancel",
      "success": "User Profile updated successfully",
      "username": {
        "label": "Username",
        "errors": {
          "required": "Please enter a username",
          "pattern": "$t(profile.profile.username.errors.required)"
        }
      },
      "email_address": {
        "label": "Email Address",
        "errors": {
          "required": "Please enter an email address",
          "pattern": "$t(profile.profile.email_address.errors.required)"
        }
      },
      "display_name": {
        "label": "Display Name",
        "errors": {
          "required": "Please enter a display name",
          "pattern": "$t(profile.profile.display_name.errors.required)"
        }
      }
    },
    "password": {
      "label": "Change Password",
      "submit": "Change Password",
      "success": "Password changed successfully",
      "oldPassword": {
        "label": "Current Password",
        "errors": {
          "required": "Please enter a password",
          "pattern": "$t(profile.password.oldPassword.errors.required)"
        }
      },
      "password": {
        "label": "New Password",
        "errors": {
          "required": "Please enter a password",
          "pattern": "$t(profile.password.password.errors.required)",
          "unchanged": "New password is the same as old"
        }
      },
      "password2": {
        "label": "Repeat New Password",
        "errors": {
          "required": "Please enter a password",
          "pattern": "$t(profile.password.password2.errors.required)",
          "noMatch": "Passwords do not match"
        }
      },
      "errors": {
        "invalidPassword": "The old password was incorrect"
      }
    },
    "worlds": {
      "label": "Worlds",
      "new": {
        "label": "New World",
        "submit": "Create World",
        "success": "New world created",
        "name": {
          "label": "Name",
          "errors": {
            "required": "Please enter a name",
            "pattern": "$t(profile.worlds.new.name.errors.required)"
          }
        },
        "description": {
          "label": "Description"
        },
        "slug": {
          "label": "URL Slug",
          "description": "If not specified, a setting of \"{{slug}}\" will be used.",
          "errors": {
            "duplicateUrlSlug": "You already have a world with this URL Slug"
          }
        },
        "errors": {
          "failed": "An error occurred creating the new world"
        }
      }
    }
  }
}
